package com.deotis.digitalars.system.exception;

public class PollingException extends Exception{

	/**
	 * polling exception
	 */

	private static final long serialVersionUID = -1887205270258348081L;

	private final String sid;

    public String getSid() {
    	return sid;
    }

    public PollingException(String sid, String message) {
    	super(message);
        this.sid = sid;
    }

}
