/*======================================*/

.target_block {
	/* 한 줄 자르기 */
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.target_inline {
	/* 유동적 너비 자르기 */
	display: inline-block;
	width: 200px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.target_box {
	/* 여러 줄 자르기 */
	display: inline-block;
	max-width: 8em;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: normal;
	line-height: 1.15em;
	height: 3.6em;
	text-align: left;
	word-wrap: break-word;
	display: -webkit-box;
	-webkit-line-clamp: 4;
	-webkit-box-orient: vertical;
}

/*======================================*/
/* overlay */
.overlay,
.overlayWhite,
.overlayBlack {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 99999999;
}

.overlayWhite_bg {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 9999999;
	background-color: #fff;
	opacity: 0.4;
}

.overlayBlack_bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 9999999;
	background-color: #222;
	opacity: 0.4;
}
/* overlay-Loading */
.overlayWhite .loading {
	position: relative;
	width: 100%;
	height: 100%;
	z-index: 9999999999;
}

/*======================================*/
/* Button  */
button {
	-moz-appearance: none;
	-webkit-appearance: none;
	-ms-appearance: none;
	appearance: none;
	-moz-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	-webkit-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	-ms-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	text-decoration: none;
	box-shadow: none;
	cursor: pointer;
	position: relative;
	display: inline-block;
	background: transparent;
	border: 0;
	border-radius: 0;
	font-size: 1rem;
	line-height: 1.125em;
	font-weight: 700;
	color: #666;
	text-align: center;
	padding: 0.5em 1.5em;
	min-height: 3.125em;
	font-family: inherit;
}

ul.button_group {
	display: flex;
	justify-content: space-between;
	gap: 0.625em;
	table-layout: fixed;
	word-break: break-all;
	width: 100%;
}

ul.button_group > li {
	display: table-cell;
	flex: 1 1;
}

ul.button_group a {
	width: 100%;
}
ul.button_group a li.choose:active {
	outline:2px solid var(--dx--main-color);
}

.button {
	width: 100%;
	text-align: center;
	background: #eee;
	height: 100%;
	font-size: 0.9em;
	margin: 0 auto;
	padding: 1.175em 0;
	border-radius: 0.25em;
}

.special {
	background-color: #666;
	color: #ffffff;
}

:root {
	/*컨트롤 변수 코드*/
	--dx--main-color:var(--dx--bagic-color);

	/*기본 제공 색상 4가지*/
	--dx--bagic-color: rgba(0, 159, 277, 100);
	--dx--icon-bagic-color: linear-gradient(to right, rgba(0, 159, 277, 100), rgba(0, 159, 277, 100));
	--dx--border-bagic-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(0, 159, 277, 100), rgba(0, 159, 277, 100));
	
	--dx--mint-color: rgba(88, 198, 199, 100);
	--dx--icon-mint-color: linear-gradient(to right, rgba(88, 198, 199, 100), rgba(88, 198, 199, 100));
	--dx--border-mint-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(88, 198, 199, 100), rgba(88, 198, 199, 100));


	--dx--gradient-color: linear-gradient(transparent, transparent), linear-gradient(to right, rgba(255, 139, 113, 100), rgba(255, 185, 121, 100));
	--dx--icon-gradient-color: linear-gradient(to right, rgba(255, 139, 113, 100), rgba(255, 185, 121, 100));
	--dx--border-gradient-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(255, 139, 113, 100), rgba(255, 185, 121, 100));
	
	--dx--green-color: rgba(168, 211, 132, 100);
	--dx--icon-green-color: linear-gradient(to right, rgba(168, 211, 132, 100), rgba(168, 211, 132, 100));
	--dx--border-green-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(168, 211, 132, 100), rgba(168, 211, 132, 100));

	--dx--customize-color: linear-gradient(transparent, transparent), linear-gradient(to right, rgba(168, 211, 132, 100), rgba(0, 159, 277, 100));
	--dx--icon-customize-color: linear-gradient(to right, rgba(168, 211, 132, 100), rgba(0, 159, 277, 100));
	--dx--border-customize-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(168, 211, 132, 100), rgba(0, 159, 277, 100));

	--dx--customize-single-color: rgba(0, 159, 277, 100);
	--dx--icon-customize-single-color: linear-gradient(to right, rgba(0, 159, 277, 100), rgba(0, 159, 277, 100));
	--dx--border-customize-single-color: linear-gradient(#fff, #fff), linear-gradient(to right, rgba(0, 159, 277, 100), rgba(0, 159, 277, 100));

	--ci-img: url(../images/logo-img.png);
	
}

.bg { --dx--main-color:var(--dx--bagic-color) !important; }
.mt { --dx--main-color:var(--dx--mint-color) !important; }
.gd { --dx--main-color:var(--dx--gradient-color) !important; }
.gr { --dx--main-color:var(--dx--green-color) !important;}
.customize {--dx--main-color: var(--dx--customize-color) !important;}
.customize-single {--dx--main-color: var(--dx--customize-single-color) !important;}


/*======================================*/
/* Layout ************************/
/* Page */
.ivr,
#ivr {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: transparent;
}

.header_wrapper {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 3.125em;
	background-color: transparent;
	z-index: 9999;
	margin: 0 auto;
}

.header {
	display: table;
	width: 100%;
	height: 100%;
}

.header_logo {
	display: table-cell;
	border: 0;
	outline: none 0;
	overflow: hidden;
	text-decoration: none;
	text-align: center;
	vertical-align: middle;
	padding: 0;
}

.header_logo > .label {
	display: none;
}

.ci_img:after {
	content: "";
	display: block;
	background-size: contain;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	margin: 0 auto;
	background-image: var(--ci-img);
	height: 1.313rem; /*1.25rem*/
	width: 6.374em;
}
/* 푸터 */
.footer_wrapper {
	position: fixed;
	bottom: 0;
	width: 100%;
	z-index: 9999;
}

.footerNav {
	position: relative;
	width: 100%;
	height: 100%;
	min-height: 3em;
	display: flex;
	justify-content: space-between;
	flex-direction: row;
	align-items: stretch;
	margin: 0;
	padding: 0;
	border-top: 1px solid #bbb;
}


.footer .button {
	display: flex;
	width: 100%;
	flex-direction: column;
	background-color: #fff;
	border-right: 1px solid #fff;
	padding: 0;
	font-size: 0.9em;
	border-radius: 0;
	height: auto;
	justify-content: center;
}
.footer .button.disabled {
	opacity: 0.25;
	cursor:default;
}

.footer .button.disabled:active {
	cursor:default;
	box-shadow:none;
	font-weight: normal;
	color: #666 !important;
}

.footer .button:active {
	background: var(--dx--main-color);
}


.footer .button .icon {
	display: flex;
	flex-direction: row;
	align-items: center;
	margin: 0 auto;
	gap: 0.4rem;
}

.footer .button .icon:before {
	background-size: auto 1.2em;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	margin-bottom: 0em;
	font-size: 1rem;
}

.footer .button .icon .custom_text {
	flex: 1 1;
	font-size: 0.84em;
	line-height: 1;
	font-weight: 400;
}

.footer .button:last-child {
	border-right: 0 !important;
}

